<h2>Productos</h2>
{{#if products }}
<section class="table-responsive">
<table class="table">
        <tr> <th>title</th> <th>price</th> <th>thumbnail</th> </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
            <th scope="row">{{ this.id }}</th>
            <td>{{ this.title }}</td>
            <td>${{ this.price }}</td>
            <td><img src="{{this.thumbnail}}" width="100" alt=""></td>
        </tr>
    {{/each}}
</table>
</section>
{{else}}
   <h3 class="alert alert-danger">error, lo que busca no existe</h3>
{{/if }} 

<a href="http://localhost:8080"><button class="btn btn-success mt-3 mb-5">cargar un producto nuevo</button></a>

